package classes_and_objects;

public class Student {
	String name;
	private int rollNumber;
	
	public Student(String n, int rn) {
		name = n;
		rollNumber = rn;
	}
	
	public void setrollNumber(int rn) {
		
		if(rn <= 0) {
			return;
		}
		rollNumber = rn;
	}
	
	public int getrollNumber() {
		return rollNumber;

	}
	
	
}
